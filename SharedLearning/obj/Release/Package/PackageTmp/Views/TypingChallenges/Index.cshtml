@model IEnumerable<SharedLearning.Models.TypingChallenge>

@{
    ViewBag.Title = "Index";
}

<h2>Typing Challenges</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WPM)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Accuracy)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WPM)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Accuracy)
        </td>
    </tr>
}

</table>
